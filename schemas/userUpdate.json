{
	"$id": "http://example.com/example.json",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"password": {
			"$id": "/properties/password",
			"type": "string",
			"title": "The Password Schema ",
			"default": "",
			"minLength": 5,
			"maxLength": 55,
			"examples": ["foo123"]
		},
		"firstname": {
			"$id": "/properties/firstname",
			"type": "string",
			"title": "The FirstName Schema ",
			"default": "",
			"minLength": 1,
			"maxLength": 55,
			"examples": ["Michael"]
		},
		"lastname": {
			"$id": "/properties/lastname",
			"type": "string",
			"title": "The LastName Schema ",
			"default": "",
			"minLength": 1,
			"maxLength": 55,
			"examples": ["Hueter"]
		},
		"email": {
			"$id": "/properties/email",
			"type": "string",
			"minLength": 6,
			"maxLength": 60,
			"title": "The Email Schema ",
			"default": "",
			"examples": ["john@conner.com"]
		}
	}
}
